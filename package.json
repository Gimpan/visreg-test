{
	"name": "visreg-test", 
	"version": "1.1.1",
	"description": "A universal visual regression testing tool which is quick and easy to set up with a simple and convenient UI to run and assess visual regression.",
	"main": "dist/run-test.js",
	"types": "dist/types.d.ts",
	"scripts": {
		"dev": "npm run watch",
		"build": "tsc",
		"watch": "tsc --watch",
		"create-symlink": "node -e \"require('fs').symlinkSync(path.join(process.cwd(), './dist'), process.argv[1] + '/node_modules/visreg-test/dist')\"",
		"test-dev": "ts-node ./src/visreg.ts",
		"pub": "tsc && npm publish",
		"prepublish": "npm run build"
	},
	"bin": {
		"visreg-test": "dist/visreg.js"
	},
	"author": "Christofer Haglund <cchaglund@users.noreply.github.com>",
	"license": "LGPL-3.0-only",
	"repository": {
		"type": "git",
		"url": "https://github.com/cchaglund/visreg-test"
	},
	"dependencies": {
		"cypress-image-snapshot-fork-2": "9.0.1-3",
        "local-cypress": "1.2.6"
    },
    "devDependencies": {
        "ts-node": "10.2.1",
        "typescript": "5.3.2"
    },
	"files": ["dist"]
}